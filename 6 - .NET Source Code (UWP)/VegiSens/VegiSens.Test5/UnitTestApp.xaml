<Application
    x:Class="VegiSens.Test5.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:VegiSens.Locator"
    xmlns:converter="using:VegiSens.Converter"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <converter:TemperatureConverter x:Key="TemperatureConverterKey"></converter:TemperatureConverter>
            <converter:HumidityConverter x:Key="HumidityConverterKey"></converter:HumidityConverter>
            <converter:LightConverter x:Key="LightConverterKey"></converter:LightConverter>
            <local:ViewModelLocator x:Key="ViewModelLocatorKey"></local:ViewModelLocator>
        </ResourceDictionary>
    </Application.Resources>
</Application>
